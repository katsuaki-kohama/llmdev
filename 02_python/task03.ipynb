   "source": [
    "investment = int(input(\"元金を入力してください:\"))\n",
    "annual_interest_rate = float(input(\"年利率(%)を入力してください:\"))\n",
    "years = 10\n",
    "\n",
    "for i in range(1, years + 1):\n",
    "    investment *= (1 + annual_interest_rate / 100)\n",
    "\n",
    "print(f\"{i}年後の金額: {investment:.2f}円\")\n"
   ]
  }
